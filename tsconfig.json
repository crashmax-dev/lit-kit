{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "module": "esnext",
    "lib": [
      "dom",
      "es2017"
    ],
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "name": "ts-lit-plugin"
      }
    ]
  }
}